			"name": "SuccessRegister",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"let baseUser = \"user\"\r",
							"let actualTime = Date.now();\r",
							"let user = `${baseUser}_${actualTime}`;\r",
							"let username = `${baseUser}_${actualTime}`;\r",
							"pm.environment.set(\"user\", user);\r",
							"pm.environment.set(\"username\", username);"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				},
				{
					"listen": "test",
					"script": {
						"exec": [
							"const response = pm.response.json();\r",
							"\r",
							"pm.test(\"Status code is 201\", function () {\r",
							"    pm.response.to.have.status(201);\r",
							"});\r",
							"\r",
							"pm.test(\"Response code is 2007\", function () {\r",
							"    pm.expect(response.code).to.eql(\"2007\");\r",
							"});"
						],
